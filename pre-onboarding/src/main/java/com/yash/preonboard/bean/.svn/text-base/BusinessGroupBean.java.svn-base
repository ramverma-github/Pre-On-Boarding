package com.yash.preonboard.bean;

import java.io.Serializable;

import com.google.gson.annotations.Expose;

public class BusinessGroupBean implements Serializable {
	
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	@Expose
	private Integer bgId;
	
	@Expose
	private String businessGroup;
	
	@Expose
	private String businessGroupHead;
	
	public BusinessGroupBean(Integer bgId, String businessGroup, String businessGroupHead) {
		super();
		this.bgId = bgId;
		this.businessGroup = businessGroup;
		this.businessGroupHead = businessGroupHead;
	}
	
	public Integer getBgId() {
		return bgId;
	}
	public void setBgId(Integer bgId) {
		this.bgId = bgId;
	}
	public String getBusinessGroup() {
		return businessGroup;
	}
	public void setBusinessGroup(String businessGroup) {
		this.businessGroup = businessGroup;
	}
	public String getBusinessGroupHead() {
		return businessGroupHead;
	}
	public void setBusinessGroupHead(String businessGroupHead) {
		this.businessGroupHead = businessGroupHead;
	}
	
	

}
